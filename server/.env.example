# OpenRouter Environment Variables Template
# Copy this content to your .env file and add your actual API keys

# Database Connection
DATABASE_URL="your_neon_database_url"

# JWT Secret (generate a strong random string)
JWT_SECRET="your_jwt_secret_key_here"

# OpenRouter API Key (get it from https://openrouter.ai/keys)
OPENROUTER_API_KEY="sk-or-v1-your_openrouter_api_key_here"

# Site URL for OpenRouter attribution (optional)
SITE_URL="http://localhost:5173"

# Site Name for OpenRouter attribution (optional)
SITE_NAME="QuickAI"

